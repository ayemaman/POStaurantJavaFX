<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="postaurant.UserInfoScreenController">
   <children>
      <HBox prefHeight="100.0" prefWidth="200.0" AnchorPane.bottomAnchor="1.0" AnchorPane.leftAnchor="1.0" AnchorPane.rightAnchor="1.0" AnchorPane.topAnchor="1.0">
         <children>
            <VBox alignment="CENTER_RIGHT" prefHeight="400.0" prefWidth="450.0">
               <children>
                  <HBox alignment="CENTER_RIGHT" prefHeight="100.0" prefWidth="200.0">
                     <children>
                        <Label text="ID:" textFill="RED">
                           <HBox.margin>
                              <Insets right="20.0" />
                           </HBox.margin></Label>
                        <TextField fx:id="idTextField" editable="false" focusTraversable="false" prefWidth="350.0">
                           <HBox.margin>
                              <Insets />
                           </HBox.margin></TextField>
                     </children>
                  </HBox>
                  <HBox alignment="CENTER_RIGHT" prefHeight="100.0" prefWidth="200.0">
                     <children>
                        <Label text="Name:" textFill="RED">
                           <HBox.margin>
                              <Insets right="20.0" />
                           </HBox.margin></Label>
                        <TextField fx:id="nameTextField" editable="false" focusTraversable="false" prefWidth="350.0">
                           <HBox.margin>
                              <Insets />
                           </HBox.margin></TextField>
                     </children></HBox>
                  <HBox alignment="CENTER_RIGHT" prefHeight="100.0" prefWidth="200.0">
                     <children>
                        <Label text="Surname:" textFill="RED">
                           <HBox.margin>
                              <Insets right="20.0" />
                           </HBox.margin></Label>
                        <TextField fx:id="surnameTextField" editable="false" focusTraversable="false" prefWidth="350.0" />
                     </children></HBox>
                  <HBox alignment="CENTER_RIGHT" prefHeight="100.0" prefWidth="200.0">
                     <children>
                        <Label text="Position:" textFill="RED">
                           <HBox.margin>
                              <Insets right="20.0" />
                           </HBox.margin></Label>
                        <TextField fx:id="positionTextField" editable="false" focusTraversable="false" prefWidth="350.0" />
                     </children></HBox>
               </children>
            </VBox>
            <VBox alignment="CENTER" prefHeight="400.0" prefWidth="150.0">
               <children>
                  <Button fx:id="goBackButton" mnemonicParsing="false" prefHeight="70.0" prefWidth="110.0" text="GO BACK">
                     <VBox.margin>
                        <Insets bottom="100.0" />
                     </VBox.margin></Button>
                  <Button fx:id="deleteUserButton" mnemonicParsing="false" prefHeight="70.0" prefWidth="110.0" text="DELETE">
                     <VBox.margin>
                        <Insets top="100.0" />
                     </VBox.margin></Button>
               </children></VBox>
         </children>
      </HBox>
   </children>
</AnchorPane>
